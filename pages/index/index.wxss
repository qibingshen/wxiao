/**index.wxss**/
@import "/tpls/manage/manage.wxss";
@import "/tpls/bannertap/bannertap.wxss";

.index-container{
	width: 100%;
	height: 100%;
	background: #f3f3f1;
}
.index-scroll{
	width: 100%;
	height: 100%;
}
.index-scroll .slide-image{
	width: 100%;
	height: 100%;
}



.index-list{
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	flex-wrap: wrap;
	background: #fff;
	margin-top: 10px;
}
.index-list view{
	width: 25%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-content: center;
	text-align: center;
	padding-top:10px;
}
.index-list navigator{
	width: 25%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	text-align: center;
	padding-top:10px;
}
.index-list view image{
	flex: 1;
	margin: 0 auto;
	width: 100rpx;
	height: 100rpx;
}
.index-list view text{
	white-space: nowrap;
	display: block;
	font-size: 12px;
	line-height: 80rpx;
	color: #020202;
}
.index-loading button{
 border: none !important;
}


